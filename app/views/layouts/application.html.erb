<html>
  <head>
    <title>RoR Quiz | <%= yield :title %></title>
  </head>
  <%= stylesheet_link_tag "screen"  %>
  
<body>
  <%= render :partial => "common/header" %>
  <div id="flash">
    <% flash.each do |key, value| -%>
      <div id="flash_<%= key %>"><%=h value %></div>
    <% end -%>
  </div>

  <%= yield %>
  <%= render :partial => "common/footer" %>
</body>
</html>
